{
  "name": "PatchPanel",
  "version": "0.0.4",
  "author": "Ben Shapiro, Jiffer Harriman, & Byron Becker",
  "description": "Simple library for creating multiple data channels to link web stuff together",
  "dependencies": {
    "@types/core-js": "^0.9.35",
    "@types/node": "^7.0.18",
    "@types/text-encoding": "0.0.30",
    "@types/web-bluetooth": "0.0.2",
    "@types/webmidi": "latest",
    "bufferutil": "1.2.x",
    "p5": "latest",
    "socket.io": "^1",
    "socket.io-client": "latest",
    "socket.io-p2p": "tommytee/socket.io-p2p",
    "socket.io-p2p-server": "tommytee/socket.io-p2p-server",
    "utf-8-validate": "1.2.x"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.41",
    "@types/node": "^7.0.18",
    "browserify": "latest",
    "ecstatic": "latest",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "ts-node": "^3.0.2",
    "tsify": "latest",
    "typescript": "^2.3.4"
  },
  "scripts": {
    "build:chatclient": "browserify src/clients/chat/chat_client.ts -p [ tsify  ] --debug > src/clients/chat/client_bundle.js",
    "build:p5demoslide": "browserify src/clients/p5demo/slide.ts -p [ tsify  ] --debug > src/clients/p5demo/slide_bundle.js",
    "build:p5demoscreech": "browserify src/clients/p5demo/screech.ts -p [ tsify  ] --debug > src/clients/p5demo/screech_bundle.js",
    "build:bare": "browserify src/clients/p5andBox/bare.ts -p [ tsify  ] --debug > src/clients/p5andBox/bare_bundle.js",
    "build:brightest": "browserify src/clients/p5andBox/bright.ts -p [ tsify  ] --debug > src/clients/p5andBox/bright_bundle.js",
    "build:pitchquantize": "browserify src/clients/p5andBox/pitchQuantize.ts -p [ tsify  ] --debug > src/clients/p5andBox/pitchQuantize_bundle.js",
    "build:notetofreq": "browserify src/clients/p5andBox/noteToFreq.ts -p [ tsify  ] --debug > src/clients/p5andBox/noteToFreq_bundle.js",
    "build:plotXY": "browserify src/clients/p5andBox/plotXY.ts -p [ tsify  ] --debug > src/clients/p5andBox/plotXY_bundle.js",
    "build:sequencer": "browserify src/clients/p5andBox/sequencer.ts -p [ tsify  ] --debug > src/clients/p5andBox/sequencer_bundle.js",
    "build:polysynth": "browserify src/clients/p5andBox/polysynth.ts -p [ tsify  ] --debug > src/clients/p5andBox/polysynth_bundle.js",
    "build:notemap": "browserify src/clients/p5andBox/noteMap.ts -p [ tsify  ] --debug > src/clients/p5andBox/noteMap_bundle.js",
    "build:helloml": "browserify src/clients/p5andBox/helloML.ts -p [ tsify  ] --debug > src/clients/p5andBox/helloML_bundle.js",
    "build:loop": "browserify src/clients/p5andBox/loop.ts -p [ tsify  ] --debug > src/clients/p5andBox/loop_bundle.js",
    "build:lightscale": "browserify src/clients/p5andBox/lightscale.ts -p [ tsify  ] --debug > src/clients/p5andBox/lightscale_bundle.js",
    "build:mbit": "browserify src/clients/p5andBox/mbit.ts -p [ tsify  ] --debug > src/clients/p5andBox/mbit_bundle.js",
    "build:rmbit": "browserify src/clients/uGenPages/routablembit.ts -p [ tsify  ] --debug > src/clients/uGenPages/routablembit_bundle.js",
    "build:brightestpixel": "browserify src/clients/uGenPages/brightestPixel.ts -p [ tsify  ] --debug > src/clients/uGenPages/brightestpixel_bundle.js",
    "build:classification": "browserify src/clients/uGenPages/classification.ts -p [ tsify  ] --debug > src/clients/uGenPages/classification_bundle.js",
    "build:rloop": "browserify src/clients/uGenPages/routableloop.ts -p [ tsify  ] --debug > src/clients/uGenPages/routableloop_bundle.js",
    "build:rsequencer": "browserify src/clients/uGenPages/routablesequencer.ts -p [ tsify  ] --debug > src/clients/uGenPages/routablesequencer_bundle.js",
    "build:rpolysynth": "browserify src/clients/uGenPages/routablepolysynth.ts -p [ tsify  ] --debug > src/clients/uGenPages/routablepolysynth_bundle.js",
    "build:acceldraw": "browserify src/clients/uGenPages/acceldraw.ts -p [ tsify  ] --debug > src/clients/uGenPages/acceldraw_bundle.js",
    "build:theremin": "browserify src/clients/uGenPages/theremin.ts -p [ tsify  ] --debug > src/clients/uGenPages/theremin_bundle.js",
    "build:button": "browserify src/clients/uGenPages/buttontest.ts -p [ tsify  ] --debug > src/clients/uGenPages/buttontest_bundle.js",
    "build:server": "browserify src/server.ts -p [ tsify  ] > lib/server.js",
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- src/server.js"
  },
  "compilerOptions": {
    "outDir": "lib",
    "declaration": true
  },
  "main": "lib/server.js",
  "typings": "lib/server"
}
